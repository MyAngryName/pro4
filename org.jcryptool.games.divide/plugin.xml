<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.views">
      <view
            category="org.jcryptool.games"
            class="org.jcryptool.games.divide.views.DivideView"
            icon="platform:/plugin/org.eclipse.ui/icons/full/obj16/elements_obj.png"
            id="org.jcryptool.games.divide.views.DivideView"
            name="%View.name">
      </view>
   </extension>
   <extension
         point="org.jcryptool.core.operations.games">
      <Game
            name="%Game.name"
            viewId="org.jcryptool.games.divide.views.DivideView"
            icon="platform:/plugin/org.eclipse.ui/icons/full/obj16/elements_obj.png">
      </Game>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="toolbar:org.jcryptool.games.divide.views.DivideView">
         <command
               commandId="org.jcryptool.games.divide.newGameCommand"
               icon="platform:/plugin/org.jcryptool.core.util/icons/icon_reset.png"
               style="push">
         </command>
         <command
               commandId="org.jcryptool.games.divide.saveGameCommand"
               icon="platform:/plugin/org.eclipse.ui/icons/full/etool16/save_edit.png"
               style="push">
         </command>
         <separator
               name="org.jcryptool.games.divide.separator1"
               visible="true">
         </separator>
         <command
               commandId="org.jcryptool.games.divide.undoCommand"
               icon="platform:/plugin/org.eclipse.ui/icons/full/etool16/undo_edit.png"
               style="push">
         </command>
         <command
               commandId="org.jcryptool.games.divide.redoCommand"
               icon="platform:/plugin/org.eclipse.ui/icons/full/etool16/redo_edit.png"
               style="push">
         </command>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            id="org.jcryptool.games.divide.newGameCommand"
            name="%Toolbar.NewGame">
      </command>
      <command
            id="org.jcryptool.games.divide.undoCommand"
            name="%Toolbar.Undo">
      </command>
      <command
            id="org.jcryptool.games.divide.redoCommand"
            name="%Toolbar.Redo">
      </command>
      <command
            id="org.jcryptool.games.divide.helpCommand"
            name="%Toolbar.Help">
      </command>
      <command
            id="org.jcryptool.games.divide.saveGameCommand"
            name="%Toolbar.Save">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="org.jcryptool.games.divide.handler.NewGameHandler"
            commandId="org.jcryptool.games.divide.newGameCommand">
         <enabledWhen>
            <with
                  variable="org.jcryptool.games.divide.newGameCommandState">
               <equals
                     value="enabled">
               </equals>
            </with>
         </enabledWhen>

      </handler>
      <handler
            class="org.jcryptool.games.divide.handler.UndoHandler"
            commandId="org.jcryptool.games.divide.undoCommand">
         <enabledWhen>
            <with
                  variable="org.jcryptool.games.divide.undo">
               <equals
                     value="enabled">
               </equals>
            </with>
         </enabledWhen>
      </handler>
      <handler
            class="org.jcryptool.games.divide.handler.RedoHandler"
            commandId="org.jcryptool.games.divide.redoCommand">
         <enabledWhen>
            <with
                  variable="org.jcryptool.games.divide.redo">
               <equals
                     value="enabled">
               </equals>
            </with>
         </enabledWhen>
      </handler>
      <handler
            class="org.jcryptool.games.divide.handler.SaveGameHandler"
            commandId="org.jcryptool.games.divide.saveGameCommand">
         <enabledWhen>
            <with
                  variable="org.jcryptool.games.divide.save">
               <equals
                     value="enabled">
               </equals>
            </with>
         </enabledWhen>
      </handler>
   </extension>
   <extension
         point="org.eclipse.help.contexts">
      <contexts
            file="$nl$/contexts.xml">
      </contexts>
      <contexts
            file="$nl$/contexts.xml">
      </contexts>
      <contexts
            file="$nl$/contexts.xml">
      </contexts>
   </extension>
   <extension
         point="org.eclipse.help.toc">
      <toc
            file="$nl$/toc.xml"
            primary="false">
      </toc>
   </extension>
   <extension
         point="org.eclipse.ui.services">
      <sourceProvider
            provider="org.jcryptool.games.divide.sourceProviders.NewGameStateSourceProvider">
         <variable
               name="org.jcryptool.games.divide.newGameCommandState"
               priorityLevel="workbench">
         </variable>
      </sourceProvider>
      <sourceProvider
            provider="org.jcryptool.games.divide.sourceProviders.UndoSourceProvider">
         <variable
               name="org.jcryptool.games.divide.undo"
               priorityLevel="workbench">
         </variable>
      </sourceProvider>
      <sourceProvider
            provider="org.jcryptool.games.divide.sourceProviders.RedoSourceProvider">
         <variable
               name="org.jcryptool.games.divide.redo"
               priorityLevel="workbench">
         </variable>
      </sourceProvider>
      <sourceProvider
            provider="org.jcryptool.games.divide.sourceProviders.SaveStateSourceProvider">
         <variable
               name="org.jcryptool.games.divide.save"
               priorityLevel="workbench">
         </variable>
      </sourceProvider>
   </extension>

</plugin>
